      "source": [
        "from sympy import symbols, Eq, solve\n",
        "\n",
        "x = symbols('x')\n",
        "\n",
        "\n",
        "equation = Eq(x/6 + x/12 + x/7 + 5 + x/2 + 4, x)\n",
        "\n",
        "\n",
        "solution = solve(equation, x)\n",
        "\n",
        "\n",
        "print(f\"Возраст Диофанта: {solution[0]} лет\")\n"
      ]
    }
